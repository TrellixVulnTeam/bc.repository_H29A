<addons>
<addon id="bc.repository" name="bico doce repository" version="0.0.3" provider-name="wndias">
	<extension point="xbmc.addon.repository" name="bicodoce Add-on Repository">
		<info compressed="false">http://raw.github.com/wndias/bc.repository/master/addons.xml</info>
		<checksum>http://raw.github.com/wndias/bc.repository/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.github.com/wndias/bc.repository/master/repo/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>bico doce Addon</summary>
		<description>entreteririmento para todos by dias wesley - visite nosso site www.entreterimento.net</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>

<addon id="plugin.video.bico-doce"
       name="Bico-doce"
       version="1.0"
       provider-name="bico-doce tv">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.t0mm0.common" version="2.0.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.1"/>
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.beautifulsoup4" />
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.requests" />
    <import addon="script.module.httplib2" />
    <import addon="script.module.youtube.dl" optional="true"/>
    <import addon="plugin.video.youtube" version="3.0.0"/>
    <import addon="script.module.urlresolver" optional="true"/>
    <import addon="script.module.simplejson" />
    <import addon="script.common.plugin.cache" version="2.5.2"/>
    <import addon="script.module.metahandler" version="2.5.1"/>

  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">entreterimento para todos </summary>
    <description lang="en">entreterimento online by Dias Wesley</description>
    <platform>all</platform>
  </extension>
</addon>

